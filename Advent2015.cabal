name:                Advent2015
version:             0.1.0.0
-- synopsis:            
-- description:         
-- license:             
license-file:        LICENSE
author:              Marc Zuo
maintainer:          zealotzuo@gmail.com
-- copyright:           
-- category:            
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable advent2015
  main-is:             Main.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.8 && <4.9, safe >=0.3 && <0.4, parsec >=3.1 && <3.2, split >= 0.2 && <0.3,
                       bytestring >=0.10 && <0.20, matrix >=0.3 && <0.4, pureMD5 >= 2.1 && <2.2,
                       array >=0.5 && <0.6, either-unwrap >=1.1 && <1.2, containers >= 0.5 && <0.6,
                       lens >=4.13 && <4.23, extra >=1.4 && <1.5, text >=1.2 && <1.3,
                       vector >=0.11 && <0.20, unordered-containers >=0.2 && <0.3,
                       aeson >=0.10 && <0.20
  hs-source-dirs:      src
  default-language:    Haskell2010

Test-Suite test-advent2015
  type:                detailed-0.9
  test-module:     	   Test
  build-depends:       base, Cabal >= 1.9.2
  hs-source-dirs:      test
